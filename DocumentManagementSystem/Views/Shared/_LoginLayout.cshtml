@inject SignInManager<ApplicationUser> signInManager;
@inject UserManager<ApplicationUser> userManager;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DocumentManagementSystem</title>

    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <meta http-equiv="content-type" content="text/html;charset=utf-8" />

    <meta name="description"
          content="Telnet Device Inventory application" />
    @*<title>Telnet Inventory - @Model.Title</title>*@
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    @*<script src="~/js/scripts/common/authRouth.js"></script>*@
    <!-- Main CSS-->
    <link rel="stylesheet" type="text/css" href="~/css/main.css" />
    <!-- Font-icon css-->
    <link rel="stylesheet"
          type="text/css"
          href="~/css/font-awesome.min.css" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css">
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link href="~/css/spinner.css" rel="stylesheet" />
</head>
<body>
    <div id="spinner" class="loading d-none">Loading&#8230;</div>

    <main>

        <div>
            @RenderBody()
        </div>
    </main>

    <hr />
    <!-- Essential javascripts for application to work-->
    <script src="~/js/jquery-3.2.1.min.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/main.js"></script>
    <!-- The javascript plugin to display page loading on top-->
    <script src="~/js/plugins/pace.min.js"></script>
    <!-- Page specific javascripts-->
    <script type="text/javascript" src="~/js/plugins/chart.js"></script>

    <!-- jquery table -->
    <script type="text/javascript" src="~/js/plugins/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/js/plugins/dataTables.bootstrap.min.js"></script>

    <!-- axios library -->
    <script src="~/js/plugins/axios.min.js"></script>

    <!-- Mustache template engine -->
    <script src="~/js/plugins/mustache.min.js"></script>
    <!-- common scripts -->
    <script src="~/js/scripts/common/repository.js"></script>

    @*select2*@
    <script src="~/js/select2.min.js"></script>

    <!--Date time picker-->
    <script src="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.js"></script>

    <!-- Date time picker-->
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

    <!--Bootstrap notify-->
    <script src="~/js/plugins/bootstrap-notify.min.js"></script>
    <!-- Jquery Validation-->
    <script src="~/js/plugins/jquery.validate.min.js"></script>
    <!--Set user data from local storage-->
    <script src="~/js/scripts/common/setUserData.js"></script>

    <script src="~/js/scripts/common/logout.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>

